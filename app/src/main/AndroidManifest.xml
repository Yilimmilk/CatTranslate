<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cattranslate.oxzu.yili.com.cattranslate">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="cattranslate.oxzu.yili.com.cattranslate.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cattranslate.oxzu.yili.com.cattranslate.Gwv"
            android:label="GwvActivity">
        </activity>
        <activity
            android:name="cattranslate.oxzu.yili.com.cattranslate.youdao_trans"
            android:label="youdao_transActivity">
        </activity>

</application>

</manifest>